{
  "name": "agentloop",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:unit": "jest --testPathPatterns='__tests__/unit'",
    "test:integration": "jest --testPathPatterns='__tests__/integration'",
    "test:performance": "jest --testPathPatterns='__tests__/performance'",
    "test:all": "jest --testPathPatterns='__tests__'",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:legacy": "echo 'Running legacy tests...' && npm run test:legacy:unit && npm run test:legacy:integration",
    "test:legacy:unit": "npm run build && node dist/tests/legacy/unit/test-simple.js",
    "test:legacy:integration": "npm run build && node dist/tests/legacy/integration/test-master-suite.js",
    "clean": "rmdir -rf dist",
    "prepublish": "npm run clean && npm run build",
    "test:robustness": "npm run build && node dist/tests/integration/test-master-suite.js",
    "test:robustness:quick": "npm run build && node dist/examples/test-master-suite.js --quick",
    "test:basic": "npm run build && node dist/examples/test-termination-fix.js",
    "test:failure-recovery": "npm run build && node dist/tests/integration/test-failure-recovery.js",
    "test:termination-fix": "npm run build && node dist/tests/integration/test-termination-fix.js",
    "test:complex-termination": "npm run build && node dist/tests/integration/test-complex-termination.js",
    "test-stress-workflows": "npm run build && node dist/tests/stress/test-stress-workflows.js",
    "test-rate-limited": "npm run build && node dist/tests/stress/test-rate-limited.js",
    "test-edge-cases": "npm run build && node dist/tests/edge-cases/test-edge-cases.js",
    "test-edge-cases-safe": "npm run build && node dist/tests/edge-cases/test-edge-cases-safe.js",
    "test-simple": "npm run build && node dist/tests/unit/test-simple.js",
    "debug-test": "npm run build && node dist/tests/unit/debug-test.js",
    "test-prompt-manager": "npm run build && node dist/tests/TestPromptManager.js",
    "test-stagnation-detector": "npm run build && node dist/tests/TestStagnationDetector.js",
    "test-stagnation-improvement": "npm run build && node dist/tests/TestStagnationImprovement.js",
    "test-forced-termination": "npm run build && node dist/tests/TestForcedTermination.js",
    "test-error-loop-detection": "npm run build && node dist/tests/TestErrorLoopDetection.js",
    "test-json-to-xsd": "npm run build && node dist/tests/JsonToXsd.test.js",
    "test:edge": "npm run build && node dist/examples/test-edge-cases.js",
    "test:edge:safe": "npm run build && node dist/examples/test-edge-cases-safe.js",
    "test:rate-limited": "npm run build && node dist/examples/test-rate-limited.js",
    "test:stress": "npm run build && node dist/examples/test-stress-workflows.js",
    "test:recovery": "npm run build && node dist/examples/test-failure-recovery.js",
    "demo:file": "npm run build && node dist/examples/FileManagementAgent.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.0.10",
    "jest": "^30.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "@jest/globals": "^30.0.4",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/cohere": "^1.2.10",
    "@ai-sdk/deepseek": "^0.2.16",
    "@ai-sdk/fireworks": "^0.2.16",
    "@ai-sdk/google": "^1.2.22",
    "@ai-sdk/groq": "^1.2.9",
    "@ai-sdk/mistral": "^1.2.8",
    "@ai-sdk/openai": "^1.3.23",
    "@ai-sdk/perplexity": "^1.1.9",
    "ai": "^4.3.19",
    "dotenv": "^17.2.0",
    "yaml": "^2.8.0",
    "zod": "^3.25.75",
    "zod-to-json-schema": "^3.24.6"
  }
}
